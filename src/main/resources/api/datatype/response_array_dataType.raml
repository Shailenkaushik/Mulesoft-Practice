#%RAML 1.0 DataType
 type: array
 items:
  type: object
  properties:
      student_id:
        type: number
        required: true
        example: 1
      student_name:
        type: string
        required: true
        example: "Shailen"
      student_email:
        type: string
        required: false
        example: "Shailen@gmail.com"